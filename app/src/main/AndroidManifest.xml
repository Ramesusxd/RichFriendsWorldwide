<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.richfriendsworldwide">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/home_page"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".SignconfActivity"
            android:label="@string/sign_up"
            android:parentActivityName=".SignconfActivity" />
        <activity
            android:name=".SignupActivity"
            android:label="@string/sign_up"
            android:parentActivityName=".SignupActivity" />
        <activity
            android:name=".TermsActivity"
            android:label="@string/Terms_condt"
            android:parentActivityName=".TermsActivity" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/rich_friend"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".MixersActivity"
            android:label="@string/night_Clubs"
            android:parentActivityName=".MixersActivity" />
        <activity
            android:name=".WorldActivity"
            android:label="@string/night_Clubs"
            android:parentActivityName=".WorldActivity" />
        <activity
            android:name=".AboutActivity"
            android:label="@string/get_around"
            android:parentActivityName=".AboutActivity" />
        <activity
            android:name=".Mixer1Activity"
            android:label="@string/night_Clubs"
            android:parentActivityName=".AboutActivity" />
        <activity
            android:name=".Mixer2Activity"
            android:label="@string/night_Clubs"
            android:parentActivityName=".AboutActivity" />
        <activity
            android:name=".MediaActivity"
            android:label="@string/on_line" />


    </application>

</manifest>